<div class="row">
    <div class="col s8">
        <h1>{{data.title}}</h1>
    </div>
    <div class="col s4 align-right">
        <p class="right-align">
            <b>Posted: </b>
            {{dateFromJSONDate data.createDate}}
        </p>
    </div>
</div>
<div class="row">
    <div class="col s12">
        <span>Salary: </span>
        <b>{{money data.salary}}</b>
    </div>
</div>
<hr>
<div class="row">
    <div class="col s12">
        <p>{{data.text}}</p>
    </div>
</div>
